<table>
    <thead>
      <tr>
        <th>Id</th>
        <th>Email</th>
      </tr>
    </thead>
  
    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td><%=user.id%></td>
          <td><%=user.email%></td>
        </tr>
      <% end %>
  </tbody>
</table>
  
<%= will_paginate @users %>